{
  "compilerOptions": {
    "target": "es2019",
    "module": "commonjs",
    "inlineSourceMap": true,
    "lib": ["es2019"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "removeComments": true,
    "skipLibCheck": true
  }
}
